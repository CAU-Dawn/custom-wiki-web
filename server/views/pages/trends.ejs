<!DOCTYPE HTML>

<html>
<head>
    <title><%= title %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://fonts.googleapis.com/css?family=ABeeZee|Poiret+One|Raleway:700" rel="stylesheet">
    <%include ../partials/stylesheet %>
    <%include ../partials/outindexcss %>
    <% include ../partials/ck %>
    <% include ../partials/meta %>

</head>
<body>
    <% include ../partials/nav %>
    <div class="container">
        <div class="Trends">
            <div class="page-header">
                <h2>최근 10개 활동왕</h2>
            </div>
            <div class="row hidden-md-up">
                <%var i=0;%>
                <%contents.forEach(function(item){%>
                <%i++;%>
                <div class="col-md-4" style="margin-bottom: 30px">
                    <figure class="snip1273">
                        <img class="" src="images/<%=i%>.png" alt="sample72"/>
                            <figcaption>
                              <h3 class="contents"><%-item.title%></h3>
                              <p class="float-right">activity index : <%=item.edit%></p>
                            </figcaption>
                        <a href="show?id=<%=item.title%>"></a>
                    </figure>
                </div>
                <%})%>

            </div>
        </div>
    </div>


    <% include ../partials/footer %>
    <% include ../partials/javascript %>
    <% include ../partials/swal %>

    <script>
     $(".hover").mouseleave(
    function () {
      $(this).removeClass("hover");
    }
  );
    </script>
    <script>
      $(document).ready(function(){
        $(window).bind('scroll', function() {
        var navHeight = $( window ).height() - 70;
          if ($(window).scrollTop() > navHeight) {
            $('nav').addClass('fixed');
          }
          else {
            $('nav').removeClass('fixed');
          }
        });
      });
    </script>

</body>
</html>
